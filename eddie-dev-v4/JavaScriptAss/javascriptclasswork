const week = {
    0: 'sunday',
    1: 'monday',
    2: 'tuesday',
    3: 'wednesday',
    4: 'thursday',
    5: 'friday',
    6: 'saturday'
};
function week_Day(Dyas){
    let item = Object.keys(week);
    for (let i = 0; i<item.length; i++){
        if(Dyas == item[i]){
            console.log(week[Dyas]);
        }
    
    }
    return Dyas;
    
    }


console.log(week_Day(3));


// creating a set.
const {log} = console
const emptySet = new Set()
log(emptySet)


const arr = ['mango','orange','mango','apple','apple','carrot'];

const fruit = new Set(arr);
log(fruit);

let uniqueArr =[...fruit] ;// converting array to set using spread operator and then assign it  
    log(uniqueArr);


let myFruit =[]
    for (const item of fruit ) {
        myFruit.push(item)

    }
    log(myFruit)

const list2 = ['javascript','java','pyhton','javascript','java'];
const addData = new Set(list2)

var list = ['javascript','java','pyhton','javascript','java'];

for(let list of addData){
    if(list.includes('p')){
        addData.delete(list);
    }
}